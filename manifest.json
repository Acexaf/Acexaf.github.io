{
  "manifest_version": 3,
  "name": "Extension Manager",
  "version": "1.0",
  "description": "Manage Chrome extensions",
  "permissions": [
    "management"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
